{
    "rewrites": [
      { "source": "/plinko/(.*)", "destination": "/plinko/index.html" }
    ],
    "headers": [
      {
        "source": "/plinko/(.*)\\.js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ]
  }